{
  "name": "reactnativereduxtemplate",
  "main": "index.tsx",
  "version": "1.0.0",
  "scripts": {
    "start": "npx expo start",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "checkDependencies": "npx expo install --check",
    "doctor": "npx expo-doctor",
    "prebuild": "chmod +x beforePrebuild.sh && ./beforePrebuild.sh && DEBUG=* npx expo prebuild --clean && chmod +x afterPrebuild.sh && ./afterPrebuild.sh",
    "androidPlayStoreBuildLocal": "chmod +x updateVersionCodeAndroid.sh && ./updateVersionCodeAndroid.sh && cd android && ENVFILE=.env ./gradlew bundleRelease",
    "updateAndroidVersionCodeAndName": "chmod +x updateVersionCodeAndroid.sh && ./updateVersionCodeAndroid.sh",
    "updateIosVersionCodeAndName": "chmod +x updateVersionCodeIos.sh && ./updateVersionCodeIos.sh",
    "EASDevelopmentBuildAndroidAndIos": "eas build --profile development --platform all",
    "EASProductionBuildAndroidAndIos": "chmod +x updateVersionCodeAndroid.sh && ./updateVersionCodeAndroid.sh && chmod +x updateVersionCodeIos.sh && ./updateVersionCodeIos.sh && eas build --profile production --platform all",
    "OverTheAirUpdates": "chmod +x overTheAirUpdates.sh && ./overTheAirUpdates.sh",
    "rollbackOTA": "eas update:rollback"
  },
  "dependencies": {
    "@expo/config-plugins": "~9.0.0",
    "@expo/vector-icons": "^14.0.2",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@reduxjs/toolkit": "^2.3.0",
    "@rneui/base": "^4.0.0-rc.7",
    "@rneui/themed": "^4.0.0-rc.8",
    "axios": "^1.8.2",
    "expo": "~52.0.37",
    "expo-build-properties": "^0.13.2",
    "expo-constants": "~17.0.7",
    "expo-dev-client": "~5.0.12",
    "expo-font": "~13.0.4",
    "expo-haptics": "~14.0.1",
    "expo-image": "^2.0.6",
    "expo-linking": "~7.0.5",
    "expo-router": "~4.0.17",
    "expo-secure-store": "^14.0.0",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.8",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.7",
    "react-native-avoid-softinput": "^7.0.0",
    "react-native-elements": "^3.4.3",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-redux": "^9.1.2",
    "redux-logger": "^3.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~18.3.12",
    "@types/react-redux": "^7.1.34",
    "@types/redux-logger": "^3.0.13",
    "typescript": "~5.3.3"
  },
  "private": true
}
