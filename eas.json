{
    "cli": {
        "version": ">= 12.6.2",
        "appVersionSource": "local"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "android": {
                "buildType": "apk"
            },
            "ios": {
                "simulator": true
            },
            "env": {
                "EXPO_PUBLIC_ENVIRONMENT": "production"
            }
        },
        "production": {
            "distribution": "store",
            "android": {
                "buildType": "app-bundle"
            },
            "ios": {
                "simulator": false
            },
            "autoIncrement": false,
            "env": {
                "EXPO_PUBLIC_ENVIRONMENT": "production"
            }
        }
    },
    "submit": {
        "production": {}
    }
}