{
  "expo": {
    "name": "React Native Redux Template",
    "slug": "reactnativereduxtemplate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "reactnativereduxtemplate",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.raahimkhan.reactnativereduxtemplate",
      "config": {
        "usesNonExemptEncryption": false
      },
      "entitlements": {
        "aps-environment": "development"
      },
      "buildNumber": "1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.raahimkhan.reactnativereduxtemplate",
      "versionCode": 1,
      "softwareKeyboardLayoutMode": "pan"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 26,
            "targetSdkVersion": 34
          },
          "ios": {
            "deploymentTarget": "16.0"
          }
        }
      ],
      "expo-font",
      "./android-build-optimisations.js"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "062cdf3e-ee0c-4374-8d04-01330032e6ff"
      }
    },
    "owner": "raahimkhan_",
    "runtimeVersion": "1.0.0",
    "updates": {
      "checkAutomatically": "NEVER"
    }
  }
}
